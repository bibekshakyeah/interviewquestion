public static void main(String[] args) {
    
        
        Random rand = new Random();
        int a = 0;
        int b = 0;
        do{                             //run first                
            a = rand.nextInt(401) + 100;
            b = rand.nextInt(401) + 100;
            System.out.println("B value: " + b);
        }while (a > b);     
  }
